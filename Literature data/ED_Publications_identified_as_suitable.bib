 keywords = {abstract ok;Association rule discovery;Association rules;Cutting tools;Data mining;Design;Hidden patterns;Implicit knowledge;Industrial database;Integer programming;Integer programming models;Knowledge discovery;Manufacture;Manufacturing capability;Manufacturing domains;Manufacturing system synthesis;Product design;Product development;Product feature},
