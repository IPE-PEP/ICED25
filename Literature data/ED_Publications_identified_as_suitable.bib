 keywords = {abstract ok;Conceptual design;Deep learning;Deep learning assisted topology optimization;Evolution law;Fine tuning;Iterative methods;Learn+;Learning models;Learning systems;Normalizing flow;Optimization models;Shape optimization;Surrogate modeling;Topology;Topology optimisation},
 volume = {133},
 issn = {09521976},
 journal = {Engineering Applications of Artificial Intelligence},
