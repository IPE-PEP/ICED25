 journal = {ADVANCES IN ENGINEERING SOFTWARE},
 doi = {10.1016/j.advengsoft.2018.09.001},
 file = {A view-based 3D CAD model reuse framework enabling product lifecycle reuse:Attachments/A view-based 3D CAD model reuse framework enabling product lifecycle reuse.pdf:application/pdf}
}


