 keywords = {abstract ok;association rule mining;Cost effectiveness;Customization;Data mining;Decision theory;Engineering design;Fuzzy clustering;Fuzzy sets;Manufacture;Membership functions;Pattern recognition;Product design;Product family design;Product functions;Product platform},
 volume = {2006},
 publisher = {{American Society of Mechanical Engineers (ASME)}},
 isbn = {079183784X},
 editor = {Moon, S. K. and Kumara, S.R.T. and Simpson, T. W.},
 doi = {10.1115/detc2006-99287},
 file = {Data mining and fuzzy clustering to support product family design:Attachments/Data mining and fuzzy clustering to support product family design.pdf:application/pdf}
