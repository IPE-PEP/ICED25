 keywords = {abstract ok;Conceptual estimates;Construction cost;Construction costs;Construction industry;Cost accounting;Cost benefit analysis;Costs;Fuzzy logic;Fuzzy systems;Genetic algorithm;Genetic algorithms;High order neural network;High-order neural network;Hybrid neural network;Hybrid neural networks;Neural network;neural networks;Project management},
 pages = {4224--4231},
