 keywords = {abstract ok;Adversarial networks;Artificial intelligence;Artificial intelligence in design;Artificial intelligence in designs;Computation theory;Computational creativities;Computational creativity;Data mining;Data-driven design;Design;Generative adversarial networks;Idea generation;Network visualisation;Semantic network analysis;Semantics},
 pages = {10--22},
 volume = {61},
 issn = {10473203},
 journal = {Journal of Visual Communication and Image Representation},
 doi = {10.1016/j.jvcir.2019.02.009},
